import React, { useEffect } from 'react';

const Data = () => {
    useEffect(() => {
        const [todos, setTodos] = useState([]);
        async function fetchTodos() {
         await fetch("https://dummyjson.com/todos")
         .then((res) => res.json())
         .then((data) => setTodos(data.todos))
         .catch((err) => console.log(err))
        }
        fetchTodos();
    }, [])
   
   return <div>Data</div>
  
};

export default Data;    